pkgname=xps-system
pkgver=3
pkgrel=1
pkgdesc="metapackage for my xps 13"
arch=('any')
url='https://github.com/seankhliao/pkgbuilds'
license=('MIT')
depends=('base'
    'linux'
    'linux-firmware'
    'intel-ucode'
    'wpa_supplicant')
source=('LICENSE')
sha256sums=('SKIP')

package() {
    install -Dm644 LICENSE $pkgdir/usr/share/licenses/$pkgname
}
