pkgname=ssh-agent-service
pkgver=1
pkgrel=1
pkgdesc=systemd user service files for ssh-agent
arch=('any')
url='https://github.com/seankhliao/pkgbuilds'
license=('MIT')
depends=('openssh' 'opensc')
source=('LICENSE'
    'ssh-agent.service')
sha256sums=('SKIP' 'SKIP')

package() {
    install -Dm644 LICENSE $pkgdir/usr/share/licenses/$pkgname
    install -Dm644 ssh-agent.service $pkgdir/usr/lib/systemd/ssh-agent.service
}
